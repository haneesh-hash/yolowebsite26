{
    "name": "yolo-living-backend",
    "version": "1.0.0",
    "description": "Backend for The YOLO Collective website to handle image uploads.",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "build:css": "lightningcss --minify style.css -o dist/style.css",
        "build:js": "terser script.js -o dist/script.js -c -m",
        "build": "mkdir -p dist && npm run build:css && npm run build:js",
        "lint": "eslint script.js server.js",
        "format": "prettier --write '*.{html,css,js}'"
    },
    "dependencies": {
        "cors": "^2.8.6",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.1",
        "helmet": "^8.1.0",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "eslint": "^10.0.0",
        "lightningcss-cli": "^1.31.1",
        "prettier": "^3.8.1",
        "terser": "^5.46.0"
    }
}
